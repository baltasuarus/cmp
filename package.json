{
  "name": "vcmp",
  "version": "0.0.1",
  "description": "Vonage Campaign Management Portal",
  "main": "index.js",
  "entry": [
    "regenerator-runtime/runtime",
    "index.js"
  ],
  "scripts": {
    "start": "NODE_ENV=production node build",
    "trigger": "NODE_ENV=production node build/runIndex.js",
    "dev": "NODE_ENV=development nodemon --exec babel-node src",
    "devTrigger": "NODE_ENV=development babel-node src/runIndex.js",
    "clean": "rm -rf build",
    "debug": "nodemon --inspect --exec babel-node src",
    "build": "npm run clean && babel src -s -D -d build",
    "test": "mocha \"./{,!(node_modules)/**/}*.test.js\" --require babel-polyfill --require esm",
    "decodePrivateKey": "echo $KEY_PRIVATE_KEY | base64 --decode > private.key",
    "heroku-postbuild": "npm run decodePrivateKey && npm run build"
  },
  "author": "Yee Hui Poh <yhpoh91@gmail.com>",
  "license": "ISC",
  "private": true,
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "aws-sdk": "^2.531.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-validation": "^1.0.2",
    "helmet": "^3.15.1",
    "http-status": "^1.3.1",
    "ini": "^1.3.5",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "log4js": "^5.1.0",
    "mongoose": "^5.6.4",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "pusher": "^3.0.0",
    "sequelize": "^5.19.2",
    "socket.io": "^2.2.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^24.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "esm": "^3.2.25",
    "jest": "^24.1.0",
    "mocha": "^6.0.1",
    "nodemon": "^1.18.10",
    "regenerator-runtime": "^0.13.2"
  }
}
